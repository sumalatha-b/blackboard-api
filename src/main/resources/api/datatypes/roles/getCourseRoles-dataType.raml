#%RAML 1.0 DataType
type: object
properties:
  results:
    type: array
    items:
      type: object
      properties:
        id: string
        roleId: string
        nameForCourses: string
        nameForOrganizations: string
        description: string
        custom: boolean
        actAsInstructor: boolean
        availability:
          type: object
          properties:
            available: string
  paging:
    type: object
    properties:
      nextPage: string
